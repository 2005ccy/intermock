{
  "name": "intermock",
  "version": "0.0.1",
  "description": "Automatically create mock objects from TypeScript interfaces via Faker",
  "bin": {
    "intermock": "./bin/src/cli.js"
  },
  "dependencies": {
    "command-line-args": "5.0.2",
    "command-line-usage": "5.0.5",
    "faker": "4.1.0",
    "fs-readfile-promise": "3.0.1",
    "lodash": "4.17.10",
    "typescript": "2.8.3"
  },
  "devDependencies": {
    "@types/mocha": "5.2.0",
    "@types/chai": "4.1.3",
    "@types/node": "~8.10.10",
    "@types/faker": "4.1.2",
    "@types/lodash": "4.14.109",
    "@types/fs-readfile-promise": "3.0.0",
    "mocha": "5.2.0",
    "mocha-notifier-reporter": "0.1.2",
    "chai": "4.1.2",
    "rimraf": "~2.6.2",
    "ts-jest": "~22.4.4",
    "ts-node": "6.0.5",
    "tslint": "~5.9.1",
    "tslint-microsoft-contrib": "~5.0.3",
    "tsutils": "~2.26.2",
    "watch-cli": "0.2.3"
  },
  "scripts": {
    "clean": "rimraf coverage build tmp",
    "ci": "npm run format-check && npm run test",
    "build": "tsc -p tsconfig.json",
    "dist": "tsc -p tsconfig.json --outDir ./bin --outFile ./bin/intermock.js",
    "watch": "tsc -w -p tsconfig.json",
    "lint": "tslint -t stylish --project \"tsconfig.json\"",
    "format-check": "./bin/format-check.sh",
    "format": "clang-format -i -style='{Language: JavaScript, BasedOnStyle: Google, ColumnLimit: 80}' src/*.ts test/*.ts",
    "test": "npm run test-only",
    "test-only": "mocha --reporter mocha-notifier-reporter -r ts-node/register test/**/*.spec.ts",
    "test-watch": "./node_modules/watch-cli/bin/watch -p \"src/**/*.ts\" -c \"npm run build && npm run test\""
  },
  "author": "Ryan McDermott <ryanmcdermott@google.com>",
  "license": "MIT"
}
